<form>
  <div class="form-group">
    <label for="emailLogin">Email address</label>
    <input type="email" class="form-control" id="emailLogin" aria-describedby="emailHelp" placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="passwordLogin">Password</label>
    <input type="password" class="form-control" id="passwordLogin" placeholder="Enter Password">
  </div>
  <button type="submit" id="login" class="btn btn-primary">Log In</button>
  <button href="/signup" type="button" id="signupPage" class="btn btn-primary">Sign Up</button>
</form>